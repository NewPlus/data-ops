# 환경 변수 템플릿
# 실제 값으로 변경 후 .env로 복사하여 사용하세요

# LLM API 설정
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Airflow 설정
AIRFLOW__CORE__DAGS_FOLDER=/home/ubuntu/data-ops/dags
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__WEBSERVER__EXPOSE_CONFIG=True

# 데이터 경로
DATA_RAW_PATH=/home/ubuntu/data-ops/data/raw
DATA_PROCESSED_PATH=/home/ubuntu/data-ops/data/processed
DATA_OUTPUT_PATH=/home/ubuntu/data-ops/data/output

# 로깅 설정
LOG_LEVEL=INFO
